{
  "name": "gofar",
  "version": "4.0.0",
  "description": "GO-FAR score calculator.",
  "author": "Jonathan Rubins <jonrubins@gmail.com>",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/gofar.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@analytics/segment": "1.1.3",
    "@headlessui/react": "1.6.5",
    "@remix-run/node": "1.6.1",
    "@remix-run/react": "1.6.1",
    "@remix-run/vercel": "1.6.1",
    "@vercel/node": "1.15.2",
    "analytics": "0.8.1",
    "clsx": "1.1.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-popper": "2.3.0",
    "tiny-invariant": "1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.1",
    "@remix-run/eslint-config": "1.6.1",
    "@remix-run/serve": "1.6.1",
    "@types/eslint": "8.4.3",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "10.4.7",
    "binode": "1.0.5",
    "cross-env": "7.0.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16"
  }
}
