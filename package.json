{
  "name": "gofar",
  "version": "2.0.0",
  "description": "GO-FAR score calculator.",
  "author": "Jonathan Rubins <jonrubins@gmail.com>",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "bundle:analyze": "NODE_ENV=production webpack --progress --config webpack.config.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "bundle:analyze:dev": "webpack --progress --config webpack.config.dev.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "lint:js": "eslint app buildConfig.js postcss.config.js webpack.config.dev.js webpack.config.prod.js --cache --ext=.js,.jsx",
    "lint:sass": "sass-lint -v -q",
    "lint": "yarn lint:js && yarn lint:sass",
    "precommit": "yarn lint && yarn test",
    "start": "git pull && yarn start:merge",
    "start:merge": "yarn && yarn start:offline",
    "start:debug": "DEBUG=true && yarn start",
    "start:offline": "webpack-dev-server --progress --config webpack.config.dev.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "8.6.0",
    "npm": "5.3.0",
    "yarn": "1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/gofar.git"
  },
  "license": "MIT",
  "private": true,
  "jest": {
    "cacheDirectory": "./.tmp",
    "resetMocks": true,
    "resetModules": true
  },
  "dependencies": {
    "classnames": "2.2.5",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.5",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.4",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.3",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.10",
    "dotenv": "5.0.0",
    "eslint": "4.18.1",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "nock": "9.2.1",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "react-hot-loader": "4.0.0-beta.14",
    "redux-mock-store": "1.5.1",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.2",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.11.0",
    "webpack-dev-server": "2.9.7",
    "webpack-dotenv-plugin": "2.0.2"
  }
}
