{
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true,
        "jquery": true
    },
    "globals": {

    },
    "rules": {
        "eqeqeq": 2,
        "no-undef": 2,
        "no-unused-vars": [2, { "args": "after-used" }],
        "react/no-direct-mutation-state": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-pascal-case": 2,
        "react/jsx-space-before-closing": 2,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2
    }
}
