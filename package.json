{
  "name": "gofar",
  "version": "4.0.0",
  "description": "GO-FAR score calculator.",
  "author": "Jonathan Rubins <jonrubins@gmail.com>",
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/gofar.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app --env production",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "postcss styles --base styles --dir app -w",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky install",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext=.js,.jsx,.ts,.tsx"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@analytics/segment": "1.1.3",
    "@headlessui/react": "1.6.5",
    "@remix-run/node": "1.6.1",
    "@remix-run/react": "1.6.1",
    "@remix-run/vercel": "1.6.1",
    "@vercel/node": "1.15.2",
    "analytics": "0.8.1",
    "clsx": "1.1.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-popper": "2.3.0",
    "tiny-invariant": "1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.6.1",
    "@remix-run/eslint-config": "1.6.1",
    "@remix-run/serve": "1.6.1",
    "@testing-library/dom": "8.14.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.2.1",
    "@types/eslint": "8.4.3",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "binode": "1.0.5",
    "cross-env": "7.0.3",
    "cssnano": "5.1.12",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "happy-dom": "6.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-cli": "10.0.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.7.2",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.1.4",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.16.0"
  },
  "engines": {
    "node": ">=16"
  }
}
