{
  "name": "gofar",
  "version": "2.0.0",
  "description": "GO-FAR score calculator.",
  "author": "Jonathan Rubins",
  "scripts": {
    "clean:deps": "rm -rf node_modules && rm -rf dist",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "bundle:analyze": "NODE_ENV=production webpack --progress --config webpack.config.prod.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "bundle:analyze:dev": "webpack --progress --config webpack.config.dev.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "deploy": "yarn clean:deps && yarn && ./deploy.sh",
    "lint:js": "eslint app buildConfig.js postcss.config.js webpack.config.dev.js webpack.config.prod.js --ext=.js,.jsx",
    "lint:sass": "sass-lint -v -q",
    "lint": "yarn lint:js && yarn lint:sass",
    "start": "git pull && yarn && webpack-dev-server --progress --config webpack.config.dev.js",
    "start:merge": "yarn && webpack-dev-server --progress --config webpack.config.dev.js",
    "start:debug": "yarn && DEBUG=true webpack-dev-server --progress --config webpack.config.dev.js",
    "start:offline": "webpack-dev-server --progress --config webpack.config.dev.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9",
    "yarn": "0.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/gofar.git"
  },
  "license": "MIT",
  "jest": {
    "cacheDirectory": "./.tmp",
    "resetMocks": true,
    "resetModules": true
  },
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "classnames": "2.2.5",
    "express": "4.14.0",
    "lodash": "4.16.4",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "serve-favicon": "2.3.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.0",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "firebase-tools": "3.6.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "node-sass": "4.5.2",
    "object-sizeof": "1.2.0",
    "postcss-loader": "1.3.3",
    "pre-commit": "1.2.2",
    "react-addons-perf": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "webpack": "2.4.1",
    "webpack-bundle-analyzer": "2.7.0",
    "webpack-dev-server": "2.4.2",
    "webpack-dotenv-plugin": "2.0.0"
  }
}
