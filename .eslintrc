{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true,
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "no-case-declarations": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-extra-boolean-cast": 2,
    "no-fallthrough": 2,
    "no-param-reassign": [2, { "props": true }],
    "no-redeclare": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-undef": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-vars": [2, { "args": "after-used" }],
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-rest-params": 2,
    "radix": 2,
    "react/forbid-component-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-string-refs": 2,
    "react/no-unknown-property": 2,
    "react/prefer-stateless-function": 2,
    "react/prop-types": 2,
    "react/sort-comp": 2,
    "spaced-comment": 2,
    "use-isnan": 2,
    "valid-jsdoc": [2, {
      "prefer": {
        "return": "returns"
      },
      "preferType": {
        "number": "Number",
        "string": "String",
        "object": "Object",
        "function": "Function"
      },
      "requireParamDescription": false,
      "requireReturn": false,
      "requireReturnDescription": false
    }],
    "valid-typeof": 2
  }
}
